{
  "name": "Twitter Emoji Extension for Twitter",
  "version": "0.1",
  "manifest_version": 2,

  "description": "Twitter Emoji Extension like Slack.",

  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "css": ["mystyles.css"],
      "js": ["jquery.min.js", "twemoji.js", "myscript.js"]
    }
  ]
}
