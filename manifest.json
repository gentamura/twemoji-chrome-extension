{
  "name": "Twemoji Extension for Twitter",
  "version": "0.1",
  "manifest_version": 2,

  "description": "Twemoji Extension like Slack.",

  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "css": ["public/css/style.css"],
      "js": ["lib/jquery.min.js", "lib/twemoji.js", "build/build.js"]
    }
  ]
}
